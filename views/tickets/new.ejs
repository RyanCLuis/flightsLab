<%- include('../partials/header') %>

<form id="newTicket" action="/flights/<%= flight._id %>/tickets" method="POST">
    <h5>Add New Ticket:</h5>
    <label>Seat:</label>
    <input name="seat" required pattern="[A-F][1-9]\d?">
    <label>Price:</label>
    <input type="Number" name="price">
    <label>Submit</label>
    <input type="submit">
</form>

<%- include('../partials/footer') %>